{
  "name": "noonic/routejs",
  "description": "Access Laravels URL/Route helper functions, from JavaScript.",
  "keywords": [
    "laravel",
    "routes",
    "routing",
    "javascript"
  ],
  "authors": [
    {
      "name": "Aaron Lord",
      "email": "hello@aaronlord.is"
    },
    {
      "name": "Sebastiano Favaro",
      "email": "s.favaro@noonic.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "illuminate/support": "6.0.*",
    "illuminate/routing": "6.0.*",
    "illuminate/console": "6.0.*",
    "illuminate/config": "6.0.*",
    "illuminate/filesystem": "6.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Noonic\\Routejs\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Noonic\\Routejs\\RoutejsServiceProvider"
      ]
    }
  },

  "minimum-stability": "dev"

}
